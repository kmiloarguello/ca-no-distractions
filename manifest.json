{
  "manifest_version": 3,
  "name": "CA No Distractions",
  "version": "1.0",
  "description": "Remove all my distractions when I am working",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://web.whatsapp.com/*"
      ]
    }
  ]
}
